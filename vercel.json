{
  "version": 2,
  "builds": [
    {
      "src": "web-build/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web-build/$1"
    },
    {
      "src": "/(.+)",
      "dest": "/web-build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web-build/index.html"
    }
  ]
}